{
  "name": "grafana4sepa",
  "version": "1.0.0",
  "description": "A Grafana Datasource Plugin that allows to visualize semantic data from a SEPA engine",
  "scripts": {
    "build": "grafana-toolkit plugin:build",
    "test": "grafana-toolkit plugin:test",
    "dev": "grafana-toolkit plugin:dev",
    "watch": "grafana-toolkit plugin:dev --watch",
    "sign": "grafana-toolkit plugin:sign",
    "start": "yarn watch"
  },
  "author": "Adelina Vivaldo",
  "maintainers": ["Gregorio Monari"],
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/runtime": "9.5.2",
    "@grafana/data": "latest",
    "@grafana/toolkit": "latest",
    "@grafana/ui": "latest",
    "@testing-library/jest-dom": "5.4.0",
    "@testing-library/react": "^10.0.2",
    "@types/lodash": "latest"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@arces-wot/sepa-js": "^0.12.1"
  }
}
